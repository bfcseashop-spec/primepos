# Copy to .env and fill in values. Do not commit .env.

# PostgreSQL connection string (required)
# If password contains @ or other special chars, URL-encode them: @ -> %40
DATABASE_URL=postgresql://user:password@localhost:5432/primepos

# Session secret (required in production). Set once and never change so login sessions survive deployments/restarts.
SESSION_SECRET=your-long-random-secret-at-least-32-chars

# Port the app listens on (default 5000; use 5010 for primepos to avoid conflict with primeclinic24)
PORT=5010
